	$(AM_V_GEN)sed \
		-e '/^##/d' \
                -e 's,[@]pkgdatadir[@],/usr/local/share/openvswitch,g' \
                -e 's,[@]RUNDIR[@],/var/run,g' \
                -e 's,[@]LOGDIR[@],/usr/local/var/log,g' \
                -e 's,[@]bindir[@],/usr/local/bin,g' \
                -e 's,[@]sysconfdir[@],/usr/local/etc,g' \
                -e 's,[@]DBDIR[@],/usr/local/etc/openvswitch,g' \
		< $? > $@.tmp && \
	mv $@.tmp $@
